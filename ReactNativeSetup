export ANDROID_HOME=/home/desktop/Android/Sdk
export PATH=$PATH:$ANDROID_HOME/tools:$ANDROID_HOME/platform-tools
echo $ANDROID_HOME

->in project
create file "local.properties":
	->local.properties

local mobile: Bus 002 Device 004: ID 1bbb:0167 T & A Mobile Phones 
echo 'SUBSYSTEM=="usb", ATTR{idVendor}=="1bbb", MODE="0666", GROUP="plugdev"' | sudo tee /etc/udev/rules.d/51-android-usb.rules
adb devices

->debbuger
adb shell input keyevent 82
	case fail execute: adb reverse tcp:8081 tcp:8081